{
  "extends": ["next/core-web-vitals", "prettier", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "no-multiple-empty-lines": ["warn", { "max": 1 }],
    "no-unused-vars": "warn",
    "semi": "warn",
    "quotes": "warn",
    "spaced-comment": "warn",
    "prefer-const": "warn",
    "max-len": ["warn", { "code": 120, "tabWidth": 2, "ignoreUrls": true }],
    "max-lines": ["warn", { "max": 35, "skipComments": true }],
    "prettier/prettier": ["warn", { "endOfLine": "auto", "singleQuote": false }]
  },
  "plugins": ["prettier"]
}